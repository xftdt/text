#include <stdio.h>

int main()
{
	char a[]="3+4";

	int x=a[0]-'0';
	int y=a[2]-'0';
	int res=x+y;

	printf("res=%d\n",res);
	return 0;
}


#include <stdio.h>
#include <string.h>
int main()
{
	char str[]="1+2+2+1+2+5+4-1-3+4-8";

	int res=str[0]-'0';

	for(int i=1;i<strlen(str);i++)
	{
		if(str[i]=='+')
		{
			int x=str[i+1]-'0';
			res=res+x;
			i++;
		}
		else if(str[i]=='-')
		{
			int x=str[i+1]-'0';
			res=res-x;
			i++;
		}
	}

	printf("res=%d\n",res);
	return 0;
}

#include <stdio.h>
#include <string.h>
int main()
{
	char str[]="2*2/4*1/1*2*3/2";

	int res=str[0]-'0';

	for(int i=1;i<strlen(str);i++)
	{
		if(str[i]=='*')
		{
			int x=str[i+1]-'0';
			res=res*x;
			i++;
		}
		else if(str[i]=='/')
		{
			int x=str[i+1]-'0';
			res=res/x;
			i++;
		}
	}

	printf("res=%d\n",res);
	return 0;
}

#include <stdio.h>
#include <string.h>
int main()
{
	char strExp[]="2+2*3+2/2-1";
	char strTmp[100];

	int strTmpIndex=-1;
	for(int i=0;i<100;i++)
	{
		if(strExp[i]=='*')
		{
			int a=strTmp[strTmpIndex]-'0';
			int b=strExp[i+1]-'0';
			strTmp[strTmpIndex]=a*b+'0';
			i++;
		}
		else if(strExp[i]=='/')
		{
			int a=strTmp[strTmpIndex]-'0';
			int b=strExp[i+1]-'0';
			strTmp[strTmpIndex]=a/b+'0';
			i++;
		}
		else strTmp[++strTmpIndex]=strExp[i];
	}

	int res=strTmp[0]-'0'; 
	for(int j=1;j<100;j++)
		{
		if(strTmp[j]=='+')
		{
			res +=strTmp[++j]-'0';
		}
		else if(strTmp[j]=='-')
		{
			res -=strTmp[++j]-'0';
		}
	}

	printf("%d\n",res);
	return 0;
}
